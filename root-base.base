views:
  - type: table
    name: Table
  - type: table
    name: lastweek
    filters:
      and:
        - file.ctime >= "2025-07-01"
  - type: table
    name: touched-last-week
    filters:
      and:
        - file.mtime >= "2025-08-15"
        - file.tags.contains("startups")
    order:
      - file.name
      - tags
  - type: table
    name: weekly
    filters:
      and:
        - file.name.startsWith("2025")
